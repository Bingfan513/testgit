<!DOCTYPE hibernate-mapping PUBLIC

        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"

        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

 

<hibernate-mapping package="cn.hh.model">

    <class name="Custom" table="custom">

        <id name="id" column="id" type="java.lang.Integer"></id>

        <property name="name"  column="name" type="java.lang.String"/>

        <property name="sex"  column="sex" type="java.lang.String"/>

        <property name="age"  column="age" type="java.lang.String"/>

    </class>

</hibernate-mapping>